<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE errorlist PUBLIC "-//Error Parser project//DTD Error Parser XML V1.0//EN"
             "http://errorparser.sourceforge.net/xml/1.0/errorParser.dtd"> 

<!-- A single group and erreur -->

<errorlist version="0.1">
  <!-- Copyright information and any additional info that will appear at 
       the top of the generated files. This is optional. -->
  <!-- The prefix will be used to label the errors (enums or defines).
       For example, the error OK would then become 
       #define TEST_OK 0 (or the enum equivalent) -->
  <prefix> test  </prefix>
  <group>
    <groupident> 
      A dummy group identifier to test if this works. 
    </groupident>
    <error>
      <number> 0 </number>
      <name> OK </name>
      <messages>
        <errormsg>No error</errormsg>
      </messages>
    </error>
    <error>
      <number> 1 </number>
      <name> INTERNAL_ERROR </name>
      <messages>
        <errormsg>Abnormal internal error - it should not happen!</errormsg>
      </messages>
    </error>
    <error>
      <number> 2 </number>
      <name> ENGINE_BUSY </name>
      <messages>
        <errormsg>
          Cannot get a lock on a system object, the engine is "busy"
        </errormsg>
        <documentation>
          <paragraph>
            This might be the result of either a very busy system where 
            unused cpu cycles are rare or a lock might be held by a 
            crashed process.
          </paragraph>
        </documentation>
      </messages>
    </error>
  </group>
  <group>
    <groupident>The following errors are network related. </groupident>
    <error>
      <number> 25 </number>
      <name> SOCKET_ERROR </name>
      <messages>
        <errormsg>
          Generic socket error.
        </errormsg>
      </messages>
    </error>
  </group>
</errorlist>

